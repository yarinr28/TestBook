{
  "name": "nest-test-e2e",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "implicitDependencies": [
      "nest-test"
    ],
    "targets": {
      "e2e": {
        "executor": "@nx/jest:jest",
        "outputs": [
          "{projectRoot}/test-output/jest/coverage"
        ],
        "options": {
          "jestConfig": "apps/nest-test-e2e/jest.config.ts",
          "passWithNoTests": true
        },
        "dependsOn": [
          "nest-test:build",
          "nest-test:serve"
        ]
      }
    }
  }
}
